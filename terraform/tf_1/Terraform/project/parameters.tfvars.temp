terraform {
  required_providers {
    aws = {
      source  = "hashicorp/aws"
      version = "~> 3.0"
    }
  }
}

provider "aws" {
  region = "us-east-1"
  access_key  = "#"
  secret_key  = "#"
}

terraform {
  backend "s3" {
    bucket = ""
    key    = "terraform.tfstate"
    region = "us-east-1"
    profile = ""
  }
}